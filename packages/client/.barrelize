{
  "$schema": "node_modules/barrelize/schema.json",
  "barrels": [
    {
      "root": "src",
      "name": "index.ts",
      "include": [
        "**/*.ts",
        "**/*.tsx"
      ],
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ],
      "replace": {
        "/\\.tsx$/": "",
        "/\\.ts$/": "",
      },
    }
  ]
}